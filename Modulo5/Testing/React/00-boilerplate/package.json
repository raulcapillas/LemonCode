{
  "name": "05-testing-01-react",
  "version": "1.0.0",
  "description": "Learn testing by sample using Jest and React, each of the samples contains a readme.md file that indicates the purpose of the sample plus an step by step guide to reproduce it.",
  "type": "module",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "vite --open",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "rimraf dist",
    "test": "jest -c ./config/test/jest.js",
    "test:watch": "npm test -- --watchAll -i"
  },
  "author": "Lemoncode",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.17.12",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.4",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
